        xmlns:local="clr-namespace:NDFD_Connect"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Name="inputBox" HorizontalAlignment="Left" Height="20" Margin="10,10,0,0" TextWrapping="Wrap" Text="Enter Address" VerticalAlignment="Top" Width="269" RenderTransformOrigin="-0.385,-0.612"/>
        <Button Click="Button_Click" Content="Get Data" HorizontalAlignment="Left" Margin="311,10,0,0" VerticalAlignment="Top" Width="75" Height="20"/>
        <TextBox Name="outputBox" HorizontalAlignment="Left" Height="267" Margin="10,42,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="497"/>

    </Grid>
</Window>
